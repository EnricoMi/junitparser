[run]
omit =
    # Don't complain if non-runnable code isn't run:
    */__main__.py,tests/**

[report]
exclude_lines =
    # Have to re-enable the standard pragma
    \#\s*pragma: no cover

    # Don't complain if non-runnable code isn't run:
    ^if __name__ == ['"]__main__['"]:$
    ^\s*if False:
